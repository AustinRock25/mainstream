    "client": "cd ui && vite",
    "seed": "dotenv -e .env -- cmd /C \"set PATH=%CD%\\sql_binaries\\bin;%PATH% && set PGPASSWORD=%DB_PASSWORD% && dropdb -h 127.0.0.1 -U %DB_USER% --if-exists %DB_NAME% && createdb -h 127.0.0.1 -U %DB_USER% %DB_NAME% && psql -h 127.0.0.1 -U %DB_USER% -d %DB_NAME% -f database.sql\""
  },
